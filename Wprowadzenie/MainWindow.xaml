<Window x:Class="Wprowadzenie.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wprowadzenie"
        mc:Ignorable="d"
        Title="Wprowadzenie" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100" />
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="2*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" />
            <RowDefinition Height="300" />
        </Grid.RowDefinitions>
        <Button Grid.Column="1"
                Grid.Row="1"
                Padding="20">1</Button>
        <Button>2</Button>
        <Button>3</Button>
        <Button>4</Button>
        <Button Grid.Column="3" Grid.Row="1">5</Button>
        <Button>5</Button>
        <Button>5</Button>
        <Button Margin="20">5</Button>

        <Canvas Grid.Column="2" Background="Aqua">
            <Rectangle Width="20" Height="50" Fill="Beige" Canvas.Left="20"></Rectangle>
        </Canvas>
    </Grid>
</Window>
